    local autoBtn=makeBtn("AUTO","â–²",Color3.fromRGB(90,50,200),0.69)

    local sf=Instance.new("Frame"); sf.Size=UDim2.new(1,-20,0,30); sf.Position=UDim2.new(0,10,0,272); sf.BackgroundColor3=Color3.fromRGB(18,24,36); sf.BackgroundTransparency=0.3; sf.Parent=AutoWalk.MainFrame
    Instance.new("UICorner",sf).CornerRadius=UDim.new(0,8); local ssb=Instance.new("UIStroke",sf); ssb.Color=Color3.fromRGB(45,65,120); ssb.Thickness=1
    AutoWalk.StatusText=Instance.new("TextLabel",sf); AutoWalk.StatusText.Size=UDim2.new(1,-10,1,0); AutoWalk.StatusText.Position=UDim2.new(0,8,0,0); AutoWalk.StatusText.BackgroundTransparency=1; AutoWalk.StatusText.Text="â¸  Idle"; AutoWalk.StatusText.TextColor3=Color3.fromRGB(140,150,180); AutoWalk.StatusText.TextSize=12; AutoWalk.StatusText.TextXAlignment=Enum.TextXAlignment.Left; AutoWalk.StatusText.Font=Enum.Font.GothamSemibold

    AutoWalk.ToggleBtn.MouseButton1Click:Connect(function()
        AutoWalk.Enabled=not AutoWalk.Enabled
        if AutoWalk.Enabled then
            AutoWalk.ToggleBtn.Text="âœ•  DISABLE AUTO WALK"
            TweenService:Create(header,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(180,40,40)}):Play()
            AutoWalk.StatusText.Text="â–²  Ready â€” Choose mode"; AutoWalk.StatusText.TextColor3=Color3.fromRGB(120,200,255)
        else
            AutoWalk.ToggleBtn.Text="â–²  ENABLE AUTO WALK"
            TweenService:Create(header,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(0,100,200)}):Play()
            aw_stop()
        end
    end)
    leftBtn.MouseButton1Click:Connect(function()
        if not AutoWalk.Enabled then return end; aw_stop(); aw_startMode("LEFT")
        AutoWalk.StatusText.TextColor3=Color3.fromRGB(80,200,120)
    end)
    rightBtn.MouseButton1Click:Connect(function()
