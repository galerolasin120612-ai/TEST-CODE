local Spin = {Enabled=false, Active=false, Speed=30, Conn=nil}
local SpinGUI = {Gui=nil, Btn=nil}

local function createSpinGUI()
    SpinGUI.Gui = Instance.new("ScreenGui")
    SpinGUI.Gui.Name = "SpinGUI"
    SpinGUI.Gui.ResetOnSpawn = false
    SpinGUI.Gui.Enabled = false
    SpinGUI.Gui.Parent = CoreGui

    SpinGUI.Btn = Instance.new("TextButton")
    SpinGUI.Btn.Size = UDim2.new(0, 148, 0, 46)
    SpinGUI.Btn.Position = UDim2.new(0.5, -74, 0.7, 0)
    SpinGUI.Btn.BackgroundColor3 = DARK_BLUE2
    SpinGUI.Btn.Text = "ðŸŒ€ SPIN: OFF"
    SpinGUI.Btn.TextColor3 = Color3.new(1,1,1)
    SpinGUI.Btn.Font = Enum.Font.GothamBold
    SpinGUI.Btn.TextSize = 13
    SpinGUI.Btn.AutoButtonColor = false
    SpinGUI.Btn.Parent = SpinGUI.Gui
    Instance.new("UICorner", SpinGUI.Btn).CornerRadius = UDim.new(0, 10)
    local ss = Instance.new("UIStroke", SpinGUI.Btn)
    ss.Color = Color3.fromRGB(30, 70, 210); ss.Thickness = 2

    addBtnDrag(SpinGUI.Btn)

    SpinGUI.Btn.MouseButton1Click:Connect(function()
        Spin.Active = not Spin.Active
        if Spin.Active then
            SpinGUI.Btn.Text = "ðŸŒ€ SPIN: ON"
            TweenService:Create(SpinGUI.Btn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(60, 20, 160)}):Play()
        else
            SpinGUI.Btn.Text = "ðŸŒ€ SPIN: OFF"
            TweenService:Create(SpinGUI.Btn, TweenInfo.new(0.15), {BackgroundColor3 = DARK_BLUE2}):Play()
        end
    end)
end

local function setupSpin(char)
    local hrp=char:WaitForChild("HumanoidRootPart")
    local e=hrp:FindFirstChild("ElevenSpin"); if e then e:Destroy() end
    local bav=Instance.new("BodyAngularVelocity"); bav.Name="ElevenSpin"
    bav.MaxTorque=Vector3.new(0,math.huge,0); bav.P=100000
    bav.AngularVelocity=Vector3.new(0,0,0); bav.Parent=hrp
end
lp.CharacterAdded:Connect(function(char) setupSpin(char) end)
if lp.Character then setupSpin(lp.Character) end

local function enableSpin()
    if not SpinGUI.Gui then createSpinGUI() end
    SpinGUI.Gui.Enabled = true
    Spin.Active = false
    SpinGUI.Btn.Text = "ðŸŒ€ SPIN: OFF"
    SpinGUI.Btn.BackgroundColor3 = DARK_BLUE2
    if Spin.Conn then Spin.Conn:Disconnect() end
    Spin.Conn=RunService.RenderStepped:Connect(function()
        local char=lp.Character; if not char then return end
        local hum=char:FindFirstChildOfClass("Humanoid")
        local hrp=char:FindFirstChild("HumanoidRootPart")
        local bav=hrp and hrp:FindFirstChild("ElevenSpin")
        if Spin.Enabled and Spin.Active and bav then
            bav.AngularVelocity=Vector3.new(0,Spin.Speed,0); if hum then hum.AutoRotate=false end
        elseif bav then
            bav.AngularVelocity=Vector3.new(0,0,0); if hum then hum.AutoRotate=true end
        end
    end)
end

local function disableSpin()
    Spin.Enabled=false; Spin.Active=false
    if SpinGUI.Gui then SpinGUI.Gui.Enabled=false end
    if SpinGUI.Btn then SpinGUI.Btn.Text="ðŸŒ€ SPIN: OFF"; SpinGUI.Btn.BackgroundColor3=DARK_BLUE2 end
    if Spin.Conn then Spin.Conn:Disconnect(); Spin.Conn=nil end
    local char=lp.Character; if char then
        local hrp=char:FindFirstChild("HumanoidRootPart")
        local bav=hrp and hrp:FindFirstChild("ElevenSpin"); if bav then bav.AngularVelocity=Vector3.new(0,0,0) end
        local hum=char:FindFirstChildOfClass("Humanoid"); if hum then hum.AutoRotate=true end
    end
end

-------------------------------------------------------------------------
-- ANTI TURRET
-------------------------------------------------------------------------
local AntiTurret={Enabled=false,Target=nil,Conn=nil,Dist=60,Pull=-5}
local function getWeapon() return lp.Backpack:FindFirstChild("Bat") or (lp.Character and lp.Character:FindFirstChild("Bat")) end
local function findTurret()
    local char=lp.Character; if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    for _,obj in pairs(Workspace:GetChildren()) do
        if obj.Name:find("Sentry") and not obj.Name:lower():find("bullet") then
            local ownerId=obj.Name:match("Sentry_(%d+)"); if ownerId and tonumber(ownerId)==lp.UserId then continue end
            local part=obj:IsA("BasePart") and obj or (obj:IsA("Model") and (obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")))
            if part and (char.HumanoidRootPart.Position-part.Position).Magnitude<=AntiTurret.Dist then return obj end
        end
    end
end

-------------------------------------------------------------------------
-- ANTI-RAGDOLL
-------------------------------------------------------------------------
local antiRagdollMode=nil; local ragdollConnections={}; local cachedCharData={}
local function cacheCharacterData()
    local char=lp.Character; if not char then return false end
    local hum=char:FindFirstChildOfClass("Humanoid"); local root=char:FindFirstChild("HumanoidRootPart")
    if not hum or not root then return false end
    cachedCharData={character=char,humanoid=hum,root=root}; return true
end
local function disconnectAllRagdoll()
    for _,conn in ipairs(ragdollConnections) do pcall(function() conn:Disconnect() end) end; ragdollConnections={}
end
local function isRagdolled()
    if not cachedCharData.humanoid then return false end
    local state=cachedCharData.humanoid:GetState()
    local rs={[Enum.HumanoidStateType.Physics]=true,[Enum.HumanoidStateType.Ragdoll]=true,[Enum.HumanoidStateType.FallingDown]=true}
    if rs[state] then return true end
    local endTime=lp:GetAttribute("RagdollEndTime")
    if endTime and (endTime-Workspace:GetServerTimeNow())>0 then return true end
    return false
end
local function forceExitRagdoll()
    if not cachedCharData.humanoid or not cachedCharData.root then return end
    pcall(function() lp:SetAttribute("RagdollEndTime",Workspace:GetServerTimeNow()) end)
    for _,d in ipairs(cachedCharData.character:GetDescendants()) do
        if d:IsA("BallSocketConstraint") or (d:IsA("Attachment") and d.Name:find("RagdollAttachment")) then d:Destroy() end
    end
    if cachedCharData.humanoid.Health>0 then cachedCharData.humanoid:ChangeState(Enum.HumanoidStateType.Running) end
    cachedCharData.root.Anchored=false
end
local function v1HeartbeatLoop() while antiRagdollMode=="v1" do task.wait(); if isRagdolled() then forceExitRagdoll() end end end
local function EnableAntiRagdoll()
    if antiRagdollMode=="v1" then return end
    if not cacheCharacterData() then return end
    antiRagdollMode="v1"
    table.insert(ragdollConnections,RunService.RenderStepped:Connect(function() local cam=Workspace.CurrentCamera; if cam and cachedCharData.humanoid then cam.CameraSubject=cachedCharData.humanoid end end))
    table.insert(ragdollConnections,lp.CharacterAdded:Connect(function() task.wait(0.5); cacheCharacterData() end))
    task.spawn(v1HeartbeatLoop)
end
local function DisableAntiRagdoll() antiRagdollMode=nil; disconnectAllRagdoll(); cachedCharData={} end

-------------------------------------------------------------------------
-- AUTO WALK â€” Triangle fixed via Heartbeat
-------------------------------------------------------------------------
local AutoWalk = {
    Enabled=false, Active=false, Mode="IDLE",
    Path1={Vector3.new(-470.6,-5.9,34.4),Vector3.new(-484.2,-3.9,21.4),Vector3.new(-475.6,-5.8,29.3),Vector3.new(-473.4,-5.9,111)},
    Path2={Vector3.new(-474.7,-5.9,91.0),Vector3.new(-483.4,-3.9,97.3),Vector3.new(-474.7,-5.9,91.0),Vector3.new(-476.1,-5.5,25.4)},
    Speed1=58.37, Speed2=29.4, JumpPower=50,
    Connection=nil, CurrentPath=nil, PathIndex=1,
    Gui=nil, MainFrame=nil, ToggleBtn=nil, StatusText=nil,
    UpTriangle=nil, TrianglePulse=nil
}

local function aw_moveToPoint(hrp,target,speed)
    local pos=hrp.Position; local dir=(target-pos)*Vector3.new(1,0,1)
    if dir.Magnitude<=3 then hrp.AssemblyLinearVelocity=Vector3.new(0,hrp.AssemblyLinearVelocity.Y,0); return true end
    local moveDir=dir.Unit*speed; hrp.AssemblyLinearVelocity=Vector3.new(moveDir.X,hrp.AssemblyLinearVelocity.Y,moveDir.Z); return false
end

local function aw_runPath(path)
    local char=lp.Character; if not char then return end
    local hrp=char:FindFirstChild("HumanoidRootPart"); if not hrp then return end
    for i=1,#path do
        if not AutoWalk.Active or AutoWalk.Mode=="IDLE" then break end
        local target=path[i]
        while AutoWalk.Active and AutoWalk.Mode~="IDLE" do
            local reached=aw_moveToPoint(hrp,target,i>2 and AutoWalk.Speed2 or AutoWalk.Speed1)
            if reached then break end
            RunService.Heartbeat:Wait()
        end
        if i==2 and AutoWalk.Active and AutoWalk.Mode~="IDLE" then
            hrp.AssemblyLinearVelocity=Vector3.new(hrp.AssemblyLinearVelocity.X,AutoWalk.JumpPower,hrp.AssemblyLinearVelocity.Z)
            task.wait(0.35)
        end
        task.wait(0.1)
    end
    if AutoWalk.Active and AutoWalk.Mode=="AUTO" then
        task.spawn(function()
            task.wait(0.5)
            if AutoWalk.Active and AutoWalk.Mode=="AUTO" then
                local c=lp.Character
                if c and c:FindFirstChild("HumanoidRootPart") then
                    local h=c.HumanoidRootPart
                    aw_runPath((h.Position.Z<60) and AutoWalk.Path2 or AutoWalk.Path1)
                end
            end
        end)
    elseif AutoWalk.Active then
        AutoWalk.Active=false; AutoWalk.Mode="IDLE"
        if AutoWalk.StatusText then AutoWalk.StatusText.Text="â¸  Idle"; AutoWalk.StatusText.TextColor3=Color3.fromRGB(140,150,180) end
    end
end

local function aw_stop()
    AutoWalk.Active=false; AutoWalk.Mode="IDLE"
    -- Stop triangle pulse
    if AutoWalk.TrianglePulse then AutoWalk.TrianglePulse:Disconnect(); AutoWalk.TrianglePulse=nil end
    if AutoWalk.UpTriangle then AutoWalk.UpTriangle.TextColor3=Color3.fromRGB(60,80,160) end
    if AutoWalk.StatusText then AutoWalk.StatusText.Text="â¸  Idle"; AutoWalk.StatusText.TextColor3=Color3.fromRGB(140,150,180) end
    local char=lp.Character
    if char and char:FindFirstChild("HumanoidRootPart") then char.HumanoidRootPart.AssemblyLinearVelocity=Vector3.zero end
end

local function aw_startMode(mode)
    if not AutoWalk.Enabled then return end
    AutoWalk.Active=true; AutoWalk.Mode=mode
    if AutoWalk.StatusText then
        if mode=="LEFT" then AutoWalk.StatusText.Text="â—€  LEFT PATH â€” Walking..."
        elseif mode=="RIGHT" then AutoWalk.StatusText.Text="â–¶  RIGHT PATH â€” Walking..."
        elseif mode=="AUTO" then AutoWalk.StatusText.Text="â–²  AUTO MODE â€” Walking..." end
    end
    -- â–² Triangle Heartbeat pulse (FIXED â€” no more recursive delay)
    if AutoWalk.TrianglePulse then AutoWalk.TrianglePulse:Disconnect(); AutoWalk.TrianglePulse=nil end
    if AutoWalk.UpTriangle then
        AutoWalk.TrianglePulse = RunService.Heartbeat:Connect(function()
            if not AutoWalk.Active then
                if AutoWalk.UpTriangle then AutoWalk.UpTriangle.TextColor3=Color3.fromRGB(60,80,160) end
                if AutoWalk.TrianglePulse then AutoWalk.TrianglePulse:Disconnect(); AutoWalk.TrianglePulse=nil end
                return
            end
            local pulse=math.abs(math.sin(tick()*math.pi*0.8))
            AutoWalk.UpTriangle.TextColor3=Color3.fromRGB(
                math.floor(40+pulse*60),
                math.floor(80+pulse*120),
                math.floor(160+pulse*95)
            )
        end)
    end
    task.spawn(function()
        if mode=="LEFT" then aw_runPath(AutoWalk.Path1)
        elseif mode=="RIGHT" then aw_runPath(AutoWalk.Path2)
        elseif mode=="AUTO" then
            local char=lp.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                aw_runPath((char.HumanoidRootPart.Position.Z<60) and AutoWalk.Path2 or AutoWalk.Path1)
            end
        end
    end)
end

local function createAutoWalkUI()
    AutoWalk.Gui=Instance.new("ScreenGui"); AutoWalk.Gui.Name="AutoWalkGUI"
    AutoWalk.Gui.ResetOnSpawn=false; AutoWalk.Gui.Enabled=false; AutoWalk.Gui.Parent=CoreGui

    AutoWalk.MainFrame=Instance.new("Frame"); AutoWalk.MainFrame.Name="AutoWalkMainFrame"
    AutoWalk.MainFrame.Size=UDim2.new(0,270,0,295); AutoWalk.MainFrame.Position=UDim2.new(0.5,-135,0.4,0)
    AutoWalk.MainFrame.BackgroundColor3=Color3.fromRGB(14,18,26); AutoWalk.MainFrame.BackgroundTransparency=0.1
    AutoWalk.MainFrame.BorderSizePixel=0; AutoWalk.MainFrame.Active=true; AutoWalk.MainFrame.Draggable=true
    AutoWalk.MainFrame.Parent=AutoWalk.Gui
    Instance.new("UICorner",AutoWalk.MainFrame).CornerRadius=UDim.new(0,14)
    local mfS=Instance.new("UIStroke",AutoWalk.MainFrame); mfS.Color=Color3.fromRGB(70,130,255); mfS.Thickness=2

    local AB=Instance.new("Frame"); AB.Size=UDim2.new(1,0,0,4); AB.BackgroundColor3=Color3.fromRGB(60,120,255); AB.BorderSizePixel=0; AB.Parent=AutoWalk.MainFrame
    Instance.new("UICorner",AB).CornerRadius=UDim.new(0,14)
    local ag=Instance.new("UIGradient",AB); ag.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(40,100,255)),ColorSequenceKeypoint.new(0.5,Color3.fromRGB(140,80,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(40,100,255))})

    local tf=Instance.new("Frame"); tf.Size=UDim2.new(1,0,0,38); tf.Position=UDim2.new(0,0,0,8); tf.BackgroundTransparency=1; tf.Parent=AutoWalk.MainFrame
    local ti=Instance.new("TextLabel",tf); ti.Size=UDim2.new(0,28,1,0); ti.Position=UDim2.new(0,12,0,0); ti.BackgroundTransparency=1; ti.Text="ðŸ¤–"; ti.TextSize=18; ti.Font=Enum.Font.GothamBold; ti.TextColor3=Color3.fromRGB(120,160,255)
    local tl=Instance.new("TextLabel",tf); tl.Size=UDim2.new(1,-50,1,0); tl.Position=UDim2.new(0,42,0,0); tl.BackgroundTransparency=1; tl.Text="Auto Walk"; tl.TextColor3=Color3.fromRGB(255,255,255); tl.TextSize=15; tl.TextXAlignment=Enum.TextXAlignment.Left; tl.Font=Enum.Font.GothamBold

    -- â–² Up Triangle Indicator
    local UT=Instance.new("TextLabel"); UT.Size=UDim2.new(0,32,0,32); UT.Position=UDim2.new(1,-44,0,6)
    UT.BackgroundTransparency=1; UT.Text="â–²"; UT.TextColor3=Color3.fromRGB(60,80,160); UT.TextSize=22; UT.Font=Enum.Font.GothamBold; UT.Parent=AutoWalk.MainFrame
    AutoWalk.UpTriangle=UT

    local header=Instance.new("Frame"); header.Size=UDim2.new(1,-20,0,40); header.Position=UDim2.new(0,10,0,52); header.BackgroundColor3=Color3.fromRGB(0,100,200); header.BorderSizePixel=0; header.Parent=AutoWalk.MainFrame
    Instance.new("UICorner",header).CornerRadius=UDim.new(0,8)
    local hg=Instance.new("UIGradient",header); hg.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(0,90,190)),ColorSequenceKeypoint.new(1,Color3.fromRGB(50,50,190))}); hg.Rotation=90
    AutoWalk.ToggleBtn=Instance.new("TextButton"); AutoWalk.ToggleBtn.Size=UDim2.new(1,0,1,0); AutoWalk.ToggleBtn.BackgroundTransparency=1
    AutoWalk.ToggleBtn.Text="â–²  ENABLE AUTO WALK"; AutoWalk.ToggleBtn.TextColor3=Color3.fromRGB(255,255,255); AutoWalk.ToggleBtn.TextSize=14; AutoWalk.ToggleBtn.Font=Enum.Font.GothamBold; AutoWalk.ToggleBtn.Parent=header

    local function createSlider(icon,label,min,max,default,yPos,callback)
        local c=Instance.new("Frame"); c.Size=UDim2.new(1,-20,0,44); c.Position=UDim2.new(0,10,0,yPos); c.BackgroundColor3=Color3.fromRGB(20,26,38); c.BackgroundTransparency=0.3; c.Parent=AutoWalk.MainFrame
        Instance.new("UICorner",c).CornerRadius=UDim.new(0,8)
        local cs=Instance.new("UIStroke",c); cs.Color=Color3.fromRGB(45,65,120); cs.Thickness=1; cs.Transparency=0.5
        local il=Instance.new("TextLabel",c); il.Size=UDim2.new(0,22,1,0); il.Position=UDim2.new(0,8,0,0); il.BackgroundTransparency=1; il.Text=icon; il.TextSize=14; il.TextColor3=Color3.fromRGB(100,160,255); il.Font=Enum.Font.GothamBold
        local lt=Instance.new("TextLabel",c); lt.Size=UDim2.new(0.55,0,1,0); lt.Position=UDim2.new(0,32,0,0); lt.BackgroundTransparency=1; lt.Text=label; lt.TextColor3=Color3.fromRGB(210,215,230); lt.TextSize=13; lt.TextXAlignment=Enum.TextXAlignment.Left; lt.Font=Enum.Font.GothamSemibold
        local vb=Instance.new("TextBox",c); vb.Size=UDim2.new(0,70,0,28); vb.Position=UDim2.new(1,-78,0.5,-14); vb.BackgroundColor3=Color3.fromRGB(10,14,26); vb.Text=tostring(default); vb.TextColor3=Color3.fromRGB(100,190,255); vb.TextSize=13; vb.Font=Enum.Font.GothamBold; vb.ClearTextOnFocus=false
        Instance.new("UICorner",vb).CornerRadius=UDim.new(0,6); local vs=Instance.new("UIStroke",vb); vs.Color=Color3.fromRGB(50,100,200); vs.Thickness=1
        vb.FocusLost:Connect(function()
            local num=tonumber(vb.Text)
            if num then num=math.clamp(num,min,max); vb.Text=tostring(num); callback(num)
            else vb.Text=tostring(default); callback(default) end
        end)
        return vb
    end
    createSlider("â–²","Speed Phase 1",1,200,AutoWalk.Speed1,100,function(v) AutoWalk.Speed1=v end)
