-- ─── Toggle function (updated colors) ─────────────────────────────────────
local function makeToggle(parent, txt, y, cb, def)
    local c = new("Frame", {
        Size = UDim2.new(1, -16, 0, 28),
        Position = UDim2.new(0, 8, 0, y),
        BackgroundTransparency = 1,
        Parent = parent
    })

    local b = new("Frame", {
        Size = UDim2.new(1, 0, 1, 0),
        BackgroundColor3 = bg_dark,
        BorderSizePixel = 0,
        Parent = c
    })
    new("UICorner", { CornerRadius = UDim.new(0, 8), Parent = b })

    local lbl = new("TextLabel", {
        Size = UDim2.new(0.64, 0, 1, 0),
        Position = UDim2.new(0, 12, 0, 0),
        Text = txt,
        TextColor3 = text_main,
        Font = Enum.Font.GothamSemibold,
        TextSize = 11,
        BackgroundTransparency = 1,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = b
    })

    local tbg = new("Frame", {
        Size = UDim2.new(0, 36, 0, 18),
        Position = UDim2.new(1, -46, 0.5, -9),
        BackgroundColor3 = Color3.fromRGB(225,225,230),
        Parent = b
    })
    new("UICorner", { CornerRadius = UDim.new(1,0), Parent = tbg })

    local knob = new("Frame", {
        Size = UDim2.new(0, 14, 0, 14),
        Position = UDim2.new(0, 2, 0.5, -7),
        BackgroundColor3 = white,
        Parent = tbg
    })
    new("UICorner", { CornerRadius = UDim.new(1,0), Parent = knob })
    new("UIStroke", { Color = border_color, Thickness = 1, Parent = knob })

    local btn = new("TextButton", {
        Size = UDim2.new(1,0,1,0),
        BackgroundTransparency = 1,
        Text = "",
        Parent = tbg
    })

    local active = def or false

    local function update()
        if active then
            ts:Create(knob, TweenInfo.new(0.18, Enum.EasingStyle.Quint), {
                Position = UDim2.new(1, -16, 0.5, -7),
                BackgroundColor3 = accent
            }):Play()
            ts:Create(tbg, TweenInfo.new(0.18), {
                BackgroundColor3 = Color3.fromRGB(220,235,255)
            }):Play()
        else
            ts:Create(knob, TweenInfo.new(0.18, Enum.EasingStyle.Quint), {
                Position = UDim2.new(0, 2, 0.5, -7),
                BackgroundColor3 = white
            }):Play()
            ts:Create(tbg, TweenInfo.new(0.18), {
                BackgroundColor3 = Color3.fromRGB(225,225,230)
            }):Play()
        end
        cb(active)
    end

    update()

    btn.MouseButton1Click:Connect(function()
        active = not active
        update()
    end)

    return {
        Set = function(v) active = v update() end,
        Get = function() return active end,
        Container = c
    }
end

-- Re-create toggles with new style
local speed1   = makeToggle(sprint, "SPEED [ NO STEALING ]", 34, function(v) speed55 = v    end, false)
local speed2   = makeToggle(sprint, "SPEED [ STEALING ]"   , 66, function(v) speedSteal = v end, false)
local spintog  = makeToggle(sprint, "SPINBOT"              , 98, function(v) toggleSpin(v)   end, false)
local grabtog  = makeToggle(sprint, "AUTO GRAB"            ,130, function(v)
    autograb = v
    if v then startGrab() else stopGrab() end
end, false)

local oc = new("Frame", {
    Size = UDim2.new(1, 0, 1, -36),
    Position = UDim2.new(0, 0, 0, 36),
    BackgroundTransparency = 1,
    Parent = main
})

local antitog  = makeToggle(oc, "ANTI RAGDOLL" ,  5, function(v) antirag = v if v then anti.Enable("v1") else anti.Disable() end end, false)
local floattog = makeToggle(oc, "FLOAT NEAREST", 37, function(v) floaton = v if v then startFloat() else stopFloat() end end, false)
local jmptog   = makeToggle(oc, "INF JUMP"     , 69, function(v) infjump = v end, false)
local xraytog  = makeToggle(oc, "X-RAY"        ,101, function(v) xrayToggle(v) end, false)

